{
  "name": "@moltlaunch/proof-of-agent",
  "version": "1.0.0",
  "description": "Verify AI agents before you trust them. On-chain attestations, STARK proofs, behavioral scoring.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "require": "./dist/client.js",
      "import": "./dist/client.mjs"
    },
    "./stark": {
      "types": "./dist/stark.d.ts",
      "require": "./dist/stark.js",
      "import": "./dist/stark.mjs"
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai-agent",
    "verification",
    "solana",
    "proof-of-agent",
    "attestation",
    "stark",
    "zero-knowledge",
