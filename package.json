  "homepage": "https://web-production-419d9.up.railway.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tradingstarllc/proof-of-agent"
  },
  "bugs": {
    "url": "https://github.com/tradingstarllc/proof-of-agent/issues"
  },
  "dependencies": {
    "@solana/web3.js": "^1.87.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
